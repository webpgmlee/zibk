            </tr>
            <tr>
				<td style="height:50%;"><iframe name="conInfoFrame" id="conInfoFrame" src="/inbound/consult/consultInfo.do" class="frame"></iframe></td>
				<%-- <c:if test="${chnlCode ne null}">
                	<td style="height:50%;"><iframe name="knowledgeFrame" id="knowledgeFrame" src="/inbound/knowledge/knowledge.do?keyword=&chnlCode=${chnlCode}" class="frame"></iframe></td>
            	</c:if>
            	<c:if test="${chnlCode eq null}">
            		<td style="height:50%;"><iframe name="knowledgeFrame" id="knowledgeFrame" src="/inbound/knowledge/knowledge.do?keyword=" class="frame"></iframe></td>
            	</c:if> --%>
            </tr>
