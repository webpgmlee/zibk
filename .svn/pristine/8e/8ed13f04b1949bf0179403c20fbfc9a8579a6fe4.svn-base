				<c:if test="${chnlCode ne null}">
                <td style="height:50%" rowspan="2"><iframe name="noticeFrame" id="noticeFrame" src="/inbound/notice/notice.do?chnlCode=${chnlCode}" class="frame"></iframe></td>
